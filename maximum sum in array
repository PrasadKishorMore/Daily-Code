public class Main
{
    
    //maximum sum in array
    
    
	public static void main(String[] args) {
		System.out.println("Hello World");
		
		int arr[] = {4,5,3,2};
		
		int maxsum=0;
		
		for(int i=0; i<arr.length; i++){
		    
		    int sum=0;

            for(int j=i; j<arr.length; j++){
                
                 sum = sum + arr[j];
                
                maxsum = Math.max(maxsum,sum);
            }		    
		    
		}
		
		System.out.println(maxsum);
	}
}
