public class Main
{
    
    // reverse the Number with string
    
	public static void main(String[] args) {
		System.out.println("Hello World");
		
		int  num = 123;
		
		String s = "";
		
		while(num > 0){
	       
	       int d = num%10;
	       
	       s = s + Integer.toString(d);   //convert into string
	       
	       num /= 10;
	       
		}
		
		System.out.println(s);
	}
