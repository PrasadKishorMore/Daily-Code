public class Main
{
	public static void main(String[] args) {
		System.out.println("Hello World");
		
		
		int arr[] = {3,0,1};
		
		int total = arr.length * (arr.length + 1)/2;
		int sum=0;
		for(int i=0; i<arr.length; i++){
		    sum +=arr[i];
		}
		
		int val = total-sum;
		
		System.out.println("missing value:",val);
		
	}
}
